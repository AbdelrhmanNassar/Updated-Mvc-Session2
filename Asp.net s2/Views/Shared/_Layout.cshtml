<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.title</title>@*c# code*@
    <link href="~/lib//bootstrap/css/bootstrap.css" rel="stylesheet" />
</head>
<body>
    @*start header *@
    <header class="mt-5">


        <center>
            <nav>
                @*Actions so you need to do it(they also will return view)*@
                <a href="Home/Index" class="btn btn-primary">Home</a>
                <a href="Home/AboutUs" class="btn btn-outline-warning">AboutUs</a>
                <a href="Home/Privacy" class="btn btn-success">Privacy</a>
                <a href="Home/ContactUs" class="btn btn-light">ContactUs</a>
            </nav>
        </center>
    </header>
    @*end header*@
    @*start Body *@
    <div>
        @RenderBody()
    </div>
    @*End Body *@
    @*start Footer*@
    <footer>
        <center>

            <div class="mt-3">

                <p>copyRights %copy</p>
            </div>

        </center>
    </footer>

    @*End Footer*@
</body>
</html>
